use 'std'

arr = newArray(2)
print arr # [0, 0]

arr1 = [1, 2, 3]
arr2 = [4, 5, 6]

arr1 << arr2 << 7
arr << '8' >> arr2

arr = 9 >> arr

print arr1 # [ 1, 2, 3, [ 8, 4, 5, 6, 7 ] ]
print arr2 # [ 8, 4, 5, 6, 7 ]
print arr # [ 9, 0, 0, [ 8, 4, 5, 6, 7 ] ]
print arr << arr[2] # [ 9, 0, 0, [ 8, 4, 5, 6, 7 ], 0 ]
